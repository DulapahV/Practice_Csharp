<Window x:Class="Hashmap.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hashmap"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="button_add" Content="Add" HorizontalAlignment="Left" Margin="135,248,0,0" VerticalAlignment="Top" Width="125" Height="50" FontSize="18" Click="button_add_Click"/>
        <Button x:Name="button_remove" Content="Remove" HorizontalAlignment="Left" Margin="335,248,0,0" VerticalAlignment="Top" Width="125" Height="50" FontSize="18" Click="button_remove_Click"/>
        <Button x:Name="button_showAll" Content="Show All" HorizontalAlignment="Left" Margin="535,248,0,0" VerticalAlignment="Top" Width="125" Height="50" FontSize="18" Click="button_showAll_Click"/>
        <TextBox x:Name="txtValue" HorizontalAlignment="Left" Height="28" Margin="135,190,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="525" FontSize="20" Text="Value"/>
        <TextBox x:Name="txtKey" HorizontalAlignment="Left" Height="28" Margin="135,143,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="525" FontSize="20" Text="Key"/>

    </Grid>
</Window>

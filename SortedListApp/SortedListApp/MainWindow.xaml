<Window x:Class="SortedListApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SortedListApp"
        mc:Ignorable="d"
        Title="SortedListApp" Height="200" Width="550">
    <Grid>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="390,45,0,0" VerticalAlignment="Top" Width="105" Height="80" Click="button_Click"/>
        <TextBox x:Name="Keytxt" HorizontalAlignment="Left" Height="30" Margin="50,45,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="315" FontSize="18"/>
        <TextBox x:Name="Valuetxt" HorizontalAlignment="Left" Height="30" Margin="50,95,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="315" FontSize="18"/>

    </Grid>
</Window>

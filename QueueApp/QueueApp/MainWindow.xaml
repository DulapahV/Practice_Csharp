<Window x:Class="QueueApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QueueApp"
        mc:Ignorable="d"
        Title="QueueApp" Height="200" Width="550">
    <Grid>
        <Button x:Name="enqueue_button" Content="Enqueue" HorizontalAlignment="Left" Margin="55,70,0,0" VerticalAlignment="Top" Width="180" Height="45" FontSize="18" Click="enqueue_button_Click"/>
        <Button x:Name="dequeue_button" Content="Dequeue" HorizontalAlignment="Left" Margin="310,70,0,0" VerticalAlignment="Top" Width="180" Height="45" FontSize="18" Click="dequeue_button_Click"/>

    </Grid>
</Window>

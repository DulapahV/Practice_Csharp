<Window x:Class="ArrayList.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArrayList"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="button_add" Content="Add" HorizontalAlignment="Left" Margin="120,230,0,0" VerticalAlignment="Top" Width="165" Height="55" Click="button_add_Click"/>
        <Button x:Name="button_remove" Content="Remove" HorizontalAlignment="Left" Margin="320,230,0,0" VerticalAlignment="Top" Width="165" Height="55"/>
        <Button x:Name="button_showAll" Content="Show All" HorizontalAlignment="Left" Margin="520,230,0,0" VerticalAlignment="Top" Width="165" Height="55" Click="button_showAll_Click"/>

    </Grid>
</Window>
